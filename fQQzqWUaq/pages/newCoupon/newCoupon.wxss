page{
    background: #ECECEC;
}
.bespcou-head{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  box-sizing: border-box;
}
.bespcou-head > view{
  float: left;
  color: #050505;
  font-size: 28rpx;
  padding: 0 25rpx;
  box-sizing: border-box;
  height: 100%;
}
.bespcou-head > view.select{
  border-bottom: 4rpx solid #F42422;
}
.bespcoup-list{
  height: calc(100% - 91rpx);
  overflow-y: auto;
  background: transparent;
}
.bespcou-item{
  width: 690rpx;
  height: 236rpx;
  /*background-image: url('https://v.iegret.com/static/user/images/coured.png');*/
  background-size: 100% 100%;
  color: #fff;
  margin: 20rpx auto 0;
  overflow: hidden;
  position: relative;
}
.overdue.bespcou-item{
  /*background-image: url('https://v.iegret.com/static/user/images/cougray.png');*/
}
.bespcou-item:last-child{
  margin-bottom: 20rpx;
}
.bespcou-item-l{
  font-size: 72rpx;
  width: 195rpx;
  float: left;
  text-align: center;
  line-height: 210rpx;
  height: 210rpx;
}
.bespcou-item-l > text{
  font-size: 30rpx;
}
.bespcou-item-r{
  float: left;
  margin-left: 26rpx;
  width: 465rpx;
}
.bespcou-name{
  font-size: 47rpx;
  line-height: 47rpx;
  margin-top: 50rpx;
}
.bespcou-condition{
  margin-top: 38rpx;
  font-size: 20rpx;
  height: 20rpx;
  line-height: 20rpx;
}
.overtip{
  width: 90rpx;
  height: 38rpx;
  line-height: 38rpx;
  text-align: center;
  color: #fff;
  font-size: 20rpx;
  position: absolute;
  border:1rpx solid #fff;
  top: 30rpx;
  right: 45rpx;
}

.bespcou-usestore{
    position: relative;
    width: 690rpx;
    margin: 2rpx auto 0;
    padding: 16rpx 60rpx 16rpx 20rpx;
    background: url('http://v.iegret.com/static/user/images/usestorebg.png');
    background-size: 100% 100%;
    color: #ffffff;
    box-sizing: border-box;
}
.bespcou-usestore-hidden{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.userstore-overdue{
    background: #CCCCCC;
}
.usestore-right-arrow{
    position: absolute;
    top: 16rpx;
    right: 10rpx;
    color: #ffffff;
    font-size: 38rpx;
    /*transition: .1s all linear;*/
}
.usestore-right-down{
    transform: rotate(90deg);
    -webkit-transform: rotate(90deg);
}
.dispatch-coupon-left{
    float: left;
    width: 36%;
    height: 100%;
    background: url('http://v.iegret.com/static/user/images/newcoupon_bg.png') no-repeat;
    background-size: 100% 100%;
    color: #ffffff;
    line-height: 236rpx;
    text-align: center;
    box-sizing: border-box;
}
.overdue .dispatch-coupon-left{
  background: url('http://v.iegret.com/static/user/images/newcoupon_nouse_bg.png') no-repeat;
  background-size: 100% 100%;
}
.dispatch-reduce-icon{
    font-size: 44rpx;
}
.dispatch-reduce-money{
    font-size: 90rpx;
}
.dispatch-coupon-right{
    float: left;
    width: 64%;
    height: 100%;
    padding: 27rpx 30rpx 27rpx 38rpx;
    background: url('http://v.iegret.com/static/user/images/newcouponl_bg.png') no-repeat;
    background-size: 100% 100%;
    position: relative;
    border-radius: 0 12rpx 12rpx 0;
    box-sizing: border-box;
}
.dispatch-reduce-top{
    width: 100%;
    height: 74rpx;
    line-height: 74rpx;
    font-size: 32rpx;
    color: #464646;
    border-bottom: 2rpx dashed #A5A5A5;
    box-sizing: border-box;
}
.dispatch-reduce-bottom{
    margin-top: 20rpx;
}
.dispatch-reduce-tip{
    font-size: 24rpx;
    color: #8E8E8E;
    margin-bottom: 12rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.dispatch-reduce-type{
    width: 126rpx;
    height: 80rpx;
    position: absolute;
    right: 0;
    top: 0;
}
.dispatch-reduce-overdue{
    width: 150rpx;
    height: 148rpx;
    border-radius: 50%;
    position: absolute;
    right: 0;
    bottom: 0;
    background: #fff;
    z-index: 2;
}